extends layout

append head
    script(src="/plugin/jquery/dist/jquery.js")
    script(src='/javascripts/play.js' type='text/javascript')
    script(rel='javascript/text' src="/plugin/bootstrap/dist/js/bootstrap.js")

block content
    button(onmousedown="accelerate(-0.2)" onmouseup="accelerate(0.05)") ACCELERATE
    input.btn(type='button', value='Play')#playButton
    input.btn(type='button', value='Save Score' style='display:none')#saveButton